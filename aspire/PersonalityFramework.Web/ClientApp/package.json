{
  "name": "ocean-personality-dynamics",
  "version": "1.0.0",
  "description": "A web application for OCEAN Personality Dynamics",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "client:dev": "vite",
    "client:build": "tsc && vite build",
    "client:preview": "vite preview",
    "server": "nodemon server.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "dev": "concurrently \"npm run server\" \"npm run client:dev\"",
    "clean": "rimraf node_modules/.cache dist"
  },
  "author": "Your Name",
  "license": "ISC",
  "dependencies": {
    "@azure/cosmos": "^4.0.0",
    "bcryptjs": "^2.4.3",
    "chart.js": "^4.3.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "mongoose": "^7.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.9",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.0",
    "eslint": "^8.46.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "glob": "^10.3.10",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.27",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vite": "^4.4.9"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
